{{>header}}

  <table class="table table-hover">
    <thead>
      <tr>
        <th>Naam</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
{{#each list}}  
      <tr>
        <td><a href="/user/show/{{_id}}">{{username}}</a></td>

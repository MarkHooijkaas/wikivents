{{> include/page.begin}}

<form role="form" method="post">
  {{> include/form.text form.username label="Gebruikersnaam"}}
  {{> include/form.text form.email    label="email"}}
  {{> include/form.text form.city    label="Woonplaats"}}
  
  {{#unless authenticated}}
  {{> include/form.text form.password label="Kies een wachtwoord"}}
  {{/unless}}
  
  {{> include/form.submitButton}}
</form>

{{> include/page.end}}
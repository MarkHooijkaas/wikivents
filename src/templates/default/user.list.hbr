{{>include/page.begin}}

  <table class="table table-hover">
    <thead>
      <tr>
        <th>Naam</th>
        <th>Woonplaats</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
{{#each list}}  
      <tr>
        <td>{{&link}}</td>
        <td>{{city}}</td>
        <td>{{email}}</td>
      </tr>
{{/each}}
    </tbody>
  </table>
  
{{>include/page.end}}
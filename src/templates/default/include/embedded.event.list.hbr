
{{#if list}}
  <table id="{{tableid}}" class="display compact" cellspacing="0" width="100%">
    <thead>
      <tr>
        <th>Datum</th>
        <th>Tijd</th>
{{#ifLoggedIn}}
        <th>Leuk</th>
{{/ifLoggedIn}}
        <th>activiteit</th>
        <th>plaats</th>
        <th>aantal</th>
        <th>Organisator</th>
        <th>geplaatst op</th>
      </tr>
    </thead>
    <tbody>
		
{{#each list}} 
	  <tr>
        <td>{{dateFormat date "eee d MMM yyyy"}}</td>
        <td>{{time}}</td>
{{#ifLoggedIn}}
        <td>{{> include/like.button record=this}}</td>
{{/ifLoggedIn}}
        <td><a href="/event/:{{_id}}">{{title}}</a></td>
        <td>{{location}}</td>
        <td>{{guestCount}}/{{max}}</td>
        <td>{{#ifLoggedIn}} {{&join organizers "<br/>"}} {{else}} *** {{/ifLoggedIn}}</td>
        <td>{{dateFormat creationDate "eee d MMM yyyy"}}</td>
      </tr>
{{/each}}
    </tbody>
  </table>
 
{{else}}
Nog geen activiteiten
{{/if}}

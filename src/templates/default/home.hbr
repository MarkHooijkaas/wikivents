{{> include/page.header }}

<h1>Wikivents : {{user.username}}</h1>

<a href="config">Configuration</a><br> 

<h2>Users</h2>
<table>
{{#each model.users}}
<tr><td>{{username}}</td><td>{{email}}</td></tr>
{{/each}}
</table>


<h2>Events</h2>
<table>
{{#each model.events}}
<tr><td>{{title}}</td><td>{{date}}</td></tr>
{{/each}}
</table>

{{> include/page.footer }}